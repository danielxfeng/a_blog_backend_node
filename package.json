{
  "name": "backend_node",
  "version": "1.0.0",
  "description": "",
  "main": "dist/app.js",
  "scripts": {
    "test": "cross-env NODE_ENV=test mocha -r ts-node/register 'tests/**/*.test.ts'",
    "init": "ts-node src/db/dbpopulate.ts",
